{
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    }
  },
  "[go][go.mod][go.sum]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[html]": {
    "editor.tabSize": 4
  },
  "[javascript][javascriptreact][typescript][typescriptreact]": {
    "editor.autoClosingBrackets": "languageDefined"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.tabSize": 3,
    "editor.wordWrap": "on"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.tabSize": 4
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.tabSize": 4
  },
  "auto-correct.dictionary": [
    {
      "languages": ["*"],
      "useLargeList": false,
      "words": {
        "MS": "Mark Skelton",
        "accomodate": "accommodate",
        "accross": "across",
        "appologize": "apologize",
        "calender": "calendar",
        "committment": "commitment",
        "concious": "conscious",
        "deductable": "deductible",
        "definately": "definitely",
        "embarass{,ment}": "embarrass{,ment}",
        "enviroment": "environment",
        "existance": "existence",
        "grammer": "grammar",
        "independant": "independent",
        "neccessary": "necessary",
        "occassion": "occasion",
        "occured": "occurred",
        "occurence": "occurrence",
        "posession": "possession",
        "priviledge": "privilege",
        "reciept": "receipt",
        "recieve": "receive",
        "relevent": "relevant",
        "seperate": "separate",
        "teh": "the",
        "ture": "true",
        "wierd": "weird"
      }
    },
    {
      "languages": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
      ],
      "words": {
        "Null": "null",
        "RN": "ReactNode",
        "cosnt": "const",
        "functoin": "function",
        "und": "undefined"
      }
    },
    {
      "languages": ["typescript", "typescriptreact"],
      "words": {
        "kn": "unknown"
      }
    }
  ],
  "breadcrumbs.enabled": false,
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.userWords": ["Akuma", "codeowners"],
  "chat.editor.fontSize": 15,
  "cursor.chat.terminalShowHoverHint": false,
  "cursor.cpp.disabledLanguages": ["plaintext", "markdown"],
  "debug.console.fontSize": 15,
  "debug.openDebug": "openOnDebugBreak",
  "debug.toolBarLocation": "docked",
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingDelete": "always",
  "editor.autoClosingOvertype": "always",
  "editor.bracketPairColorization.enabled": false,
  "editor.codeActionsOnSave": ["source.fixAll"],
  "editor.colorDecorators": false,
  "editor.comments.ignoreEmptyLines": false,
  "editor.cursorBlinking": "phase",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.dragAndDrop": false,
  "editor.fontFamily": "JetBrains Mono, Symbols Nerd Font, codicon, termicons, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.gotoLocation.multipleDeclarations": "gotoAndPeek",
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleImplementations": "gotoAndPeek",
  "editor.gotoLocation.multipleReferences": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "editor.guides.bracketPairsHorizontal": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.highlightActiveIndentation": false,
  "editor.guides.indentation": false,
  "editor.inlayHints.enabled": "off",
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.matchBrackets": "never",
  "editor.occurrencesHighlight": "off",
  "editor.rename.enablePreview": false,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "trailing",
  "editor.rulers": [80],
  "editor.selectionHighlight": false,
  "editor.stickyScroll.enabled": false,
  "editor.suggest.preview": true,
  "editor.suggest.showWords": false,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "onlySnippets",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.allowedCharacters": {
    "–": true,
    "’": true,
    "“": true,
    "”": true
  },
  "editor.wrappingIndent": "indent",
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "emmet.triggerExpansionOnTab": true,
  "errorLens.statusBarColorsEnabled": true,
  "errorLens.statusBarIconsAtZero": "hide",
  "errorLens.statusBarIconsEnabled": true,
  "errorLens.statusBarIconsTargetProblems": "activeEditor",
  "errorLens.statusBarIconsUseBackground": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.colors": false,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.ftl": "html",
    "*.mq5": "cpp",
    "*.mqh": "cpp",
    "*.pro": "prolog",
    ".releaserc": "json",
    ".stylelintrc": "json"
  },
  "files.exclude": {
    "**/*.iml": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.gradle": true,
    "**/.idea": true,
    "**/.project": true,
    "**/.settings": true,
    "**/node_modules": true,
    "**/out": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.blame.statusBarItem.enabled": true,
  "git.branchProtection": ["main", "master"],
  "git.branchProtectionPrompt": "alwaysCommit",
  "git.checkoutType": ["local"],
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "githubIssues.issueCompletions.enabled": false,
  "githubIssues.userCompletions.enabled": false,
  "githubPullRequests.commentExpandState": "collapseAll",
  "githubPullRequests.experimental.notificationsView": true,
  "githubPullRequests.notifications": "pullRequests",
  "githubPullRequests.pullBranch": "never",
  "githubPullRequests.pullRequestDescription": "none",
  "githubPullRequests.queries": [
    {
      "label": "Created By Me",
      "query": "repo:${owner}/${repository} is:open author:${user}"
    },
    {
      "label": "Assigned To Me",
      "query": "repo:${owner}/${repository} is:open assignee:${user}"
    }
  ],
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.graph.statusBar.enabled": false,
  "gitlens.integrations.enabled": true,
  "gitlens.launchpad.indicator.enabled": false,
  "gitlens.menus": {
    "editorGroup": {
      "blame": false,
      "compare": false
    }
  },
  "gitlens.remotes": [
    {
      "domain": "github.com",
      "type": "GitHub"
    },
    {
      "regex": "([^:]+):(.+)",
      "type": "Custom",
      "urls": {
        "repository": "https://github.com/${repo}",
        "branches": "https://github.com/${repo}/branches",
        "branch": "https://github.com/${repo}/commits/${branch}",
        "commit": "https://github.com/${repo}/commit/${id}",
        "file": "https://github.com/${repo}?path=${file}${line}",
        "fileInBranch": "https://github.com/${repo}/blob/${branch}/${file}${line}",
        "fileInCommit": "https://github.com/${repo}/blob/${id}/${file}${line}",
        "fileLine": "#L${line}",
        "fileRange": "#L${start}-L${end}"
      }
    }
  ],
  "gitlens.statusBar.enabled": false,
  "gitlens.telemetry.enabled": false,
  "gitlens.terminalLinks.enabled": false,
  "gitlens.virtualRepositories.enabled": false,
  "html.format.wrapLineLength": 80,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "literals",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.preview.fontSize": 15,
  "material-icon-theme.activeIconPack": "react",
  "notebook.markup.fontSize": 15,
  "outline.showFields": false,
  "outline.showKeys": false,
  "outline.showProperties": false,
  "outline.showVariables": false,
  "playwright.reuseBrowser": false,
  "playwright.showTrace": false,
  "prettier.requireConfig": true,
  "scm.diffDecorationsGutterPattern": {
    "modified": false
  },
  "search.decorations.colors": false,
  "search.exclude": {
    "**/.yarn": true,
    "**/tsconfig.tsbuildinfo": true
  },
  "search.quickOpen.history.filterSortOrder": "recency",
  "search.seedWithNearestWord": true,
  "search.smartCase": true,
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.startupPrompt": "never",
  "terminal.external.osxExec": "Iterm.app",
  "terminal.integrated.commandsToSkipShell": ["workbench.action.openRecent"],
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.env.osx": {
    "NODE_ENV": null
  },
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.tabs.location": "left",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "literals",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.foldfix": true,
  "vim.gdefault": true,
  "vim.handleKeys": {
    "<C-r>": true,
    "<C-v>": true
  },
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    // Find
    {
      "before": ["<leader>", "f", "p"],
      "commands": ["workbench.action.quickOpen"],
      "silent": true
    },
    {
      "before": ["<leader>", "f", "d"],
      "commands": ["workbench.files.action.showActiveFileInExplorer"],
      "silent": true
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": ["workbench.action.findInFiles"],
      "silent": true
    },
    {
      "before": ["<leader>", "f", "o"],
      "commands": ["workbench.action.openRecent"],
      "silent": true
    },
    {
      "before": ["<leader>", "f", "n"],
      "commands": ["git.checkout"],
      "silent": true
    },
    {
      "before": ["<leader>", "f", "y"],
      "commands": ["workbench.action.gotoSymbol"],
      "silent": true
    },
    {
      "before": ["<leader>", "f", "a"],
      "commands": ["adjacent.quickOpen"]
    },
    {
      "before": ["<leader>", "t", "s"],
      "commands": [
        {
          "command": "workbench.action.findInFiles",
          "args": {
            "filesToInclude": "src/routes/Treasury/**,src/modules/financialProducts/**,playwright/tests/treasury/**"
          }
        }
      ],
      "silent": true
    },
    // Treat the search editor like the quick fix list
    {
      "before": ["]", "q"],
      "commands": ["search.action.focusNextSearchResult"]
    },
    {
      "before": ["[", "q"],
      "commands": ["search.action.focusPreviousSearchResult"]
    },
    // Git
    {
      "before": ["<leader>", "v", "h"],
      "commands": ["gitlens.toggleFileBlame"],
      "silent": true
    },
    {
      "before": ["<leader>", "v", "d"],
      "commands": ["gitlens.diffWithPrevious"],
      "silent": true
    },
    {
      "before": ["<leader>", "v", "p"],
      "commands": ["git.push"],
      "silent": true
    },
    {
      "before": ["<leader>", "v", "w"],
      "commands": [
        "gitlens.copyRemoteFileUrlToClipboard",
        {
          "command": "vscode-notify.sendNotification",
          "args": {
            "message": "Remote URL copied to clipboard",
            "type": "info"
          }
        }
      ],
      "silent": true
    },
    {
      "before": ["<leader>", "v", "s"],
      "commands": ["git.stageChange"],
      "silent": true
    },
    {
      "before": ["<leader>", "v", "u"],
      "commands": ["git.revertChange"],
      "silent": true
    },
    {
      "before": ["[", "v"],
      "commands": ["workbench.action.editor.previousChange"],
      "silent": true
    },
    {
      "before": ["]", "v"],
      "commands": ["workbench.action.editor.nextChange"],
      "silent": true
    },
    {
      "before": ["<leader>", "l", "g"],
      "commands": ["workbench.view.scm"],
      "silent": true
    },
    // LSP
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"],
      "silent": true
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"],
      "silent": true
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showDefinitionPreviewHover"],
      "silent": true
    },
    {
      "before": ["g", "O"],
      "commands": ["editor.action.organizeImports"],
      "silent": true
    },
    {
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"],
      "silent": true
    },
    // Navigate to next/previous editor
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.previousEditor"],
      "silent": true
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.nextEditor"],
      "silent": true
    },
    // Undo/redo
    {
      "before": ["u"],
      "commands": ["undo"],
      "silent": true
    },
    {
      "before": ["<C-r>"],
      "commands": ["redo"],
      "silent": true
    },
    // Open command line with ;
    {
      "before": [";"],
      "after": [":"],
      "silent": true
    },
    // Map leader semicolon to the original semicolon motion to still allow using
    // (next f/t match) since that motion is quite handy. I don't use it much, so
    // the extra keypress is fine.
    {
      "before": ["<leader>", ";"],
      "after": [";"],
      "silent": true
    },
    // I frequently roll zv for visually selection which doesn't work great on my
    // keyboard since I use a multi-function key for z/ctrl. This mapping sets
    // zv to the same as C-v.
    {
      "before": ["z", "v"],
      "commands": ["extension.vim_ctrl+v"],
      "silent": true
    },
    // q: is an odd key sequence, so <leader>q is easier to type
    {
      "before": ["<leader>", "q"],
      "after": ["q", ":"],
      "silent": true
    },
    // Update the Y mapping to match the Neovim behavior of "yank to end of line".
    // Yank entire line is easy with `yy` and yank to end of line is most often
    // what I want and also is consistent with my Neovim setup.
    {
      "before": ["Y"],
      "after": ["y", "$"],
      "silent": true
    },
    // Save file
    {
      "before": [",", "s"],
      "commands": ["workbench.action.files.save"],
      "silent": true
    },
    // Close window
    {
      "before": [",", "w"],
      "commands": ["workbench.action.closeActiveEditor"],
      "silent": true
    },
    // Close other editors (like Vim's :only)
    {
      "before": ["<leader>", "o", "n"],
      "commands": ["workbench.action.closeOtherEditors"],
      "silent": true
    },
    // File utils
    {
      "before": ["<leader>", "r", "f"],
      "commands": ["fileutils.renameFile"],
      "silent": true
    },
    // TODO: Confirm delete
    {
      "before": ["<leader>", "o", "d", "f"],
      "commands": ["fileutils.removeFile"],
      "silent": true
    },
    {
      "before": ["<leader>", "o", "f"],
      "commands": ["revealFileInOS"],
      "silent": true
    },
    // Make file executable
    {
      "before": ["<leader>", "o", "x"],
      "commands": [
        {
          "command": "unix-chmod.chmod",
          "args": ["+x"]
        }
      ],
      "silent": true
    },
    // Emmet
    {
      "before": ["<leader>", "e", "m"],
      "commands": ["editor.emmet.action.wrapWithAbbreviation"],
      "silent": true
    },
    // Open URL under cursor
    {
      "before": ["g", "x"],
      "commands": ["open-url.openUrlUnderCursor"],
      "silent": true
    },
    // TODO: Sort inner indent
    {
      "before": ["<leader>", "s", "i"],
      "commands": [],
      "silent": true
    },
    // Text objects
    {
      "before": ["v", "a", "f"],
      "commands": ["vscode-textobjects.select.next.outer.function.start"]
    },
    {
      "before": ["v", "i", "f"],
      "commands": ["vscode-textobjects.select.next.inner.function.start"]
    },
    {
      "before": ["v", "a", "d"],
      "commands": ["vscode-textobjects.select.next.outer.variable.start"]
    },
    {
      "before": ["v", "a", "a"],
      "commands": ["vscode-textobjects.select.next.outer.parameters.start"]
    },
    {
      "before": ["v", "i", "a"],
      "commands": ["vscode-textobjects.select.next.inner.parameters.start"]
    },
    {
      "before": ["v", "a", "c"],
      "commands": ["vscode-textobjects.select.next.outer.comment.start"]
    },
    {
      "before": ["v", "i", "c"],
      "commands": ["vscode-textobjects.select.next.inner.comment.start"]
    },
    {
      "before": ["]", "f"],
      "commands": ["vscode-textobjects.goTo.next.outer.function.start"]
    },
    {
      "before": ["]", "F"],
      "commands": ["vscode-textobjects.goTo.next.inner.function.start"]
    },
    {
      "before": ["]", "d"],
      "commands": ["vscode-textobjects.goTo.next.outer.variable.start"]
    },
    {
      "before": ["]", "a"],
      "commands": ["vscode-textobjects.goTo.next.outer.parameters.start"]
    },
    {
      "before": ["]", "A"],
      "commands": ["vscode-textobjects.goTo.next.inner.parameters.start"]
    },
    {
      "before": ["]", "c"],
      "commands": ["vscode-textobjects.goTo.next.outer.comment.start"]
    },
    {
      "before": ["]", "C"],
      "commands": ["vscode-textobjects.goTo.next.inner.comment.start"]
    },
    {
      "before": ["]", "]", "f"],
      "commands": ["vscode-textobjects.goTo.next.outer.function.end"]
    },
    {
      "before": ["]", "]", "F"],
      "commands": ["vscode-textobjects.goTo.next.inner.function.end"]
    },
    {
      "before": ["]", "]", "d"],
      "commands": ["vscode-textobjects.goTo.next.outer.variable.end"]
    },
    {
      "before": ["]", "]", "a"],
      "commands": ["vscode-textobjects.goTo.next.outer.parameters.end"]
    },
    {
      "before": ["]", "]", "A"],
      "commands": ["vscode-textobjects.goTo.next.inner.parameters.end"]
    },
    {
      "before": ["]", "]", "c"],
      "commands": ["vscode-textobjects.goTo.next.outer.comment.end"]
    },
    {
      "before": ["]", "]", "C"],
      "commands": ["vscode-textobjects.goTo.next.inner.comment.end"]
    },
    {
      "before": ["[", "f"],
      "commands": ["vscode-textobjects.goTo.previous.outer.function.start"]
    },
    {
      "before": ["[", "F"],
      "commands": ["vscode-textobjects.goTo.previous.inner.function.start"]
    },
    {
      "before": ["[", "d"],
      "commands": ["vscode-textobjects.goTo.previous.outer.variable.start"]
    },
    {
      "before": ["[", "A"],
      "commands": ["vscode-textobjects.goTo.previous.inner.parameters.start"]
    },
    {
      "before": ["[", "a"],
      "commands": ["vscode-textobjects.goTo.previous.outer.parameters.start"]
    },
    {
      "before": ["[", "c"],
      "commands": ["vscode-textobjects.goTo.previous.outer.comment.start"]
    },
    {
      "before": ["[", "C"],
      "commands": ["vscode-textobjects.goTo.previous.inner.comment.start"]
    },
    {
      "before": ["[", "[", "f"],
      "commands": ["vscode-textobjects.goTo.previous.outer.function.end"]
