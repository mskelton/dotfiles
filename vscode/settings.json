    {
      "before": ["v", "i", "F"],
      "commands": ["vscode-textobjects.select.previous.inner.function.start"]
    },
    {
      "before": ["v", "a", "D"],
      "commands": ["vscode-textobjects.select.previous.outer.variable.start"]
    },
    {
      "before": ["v", "i", "A"],
      "commands": ["vscode-textobjects.select.previous.inner.parameters.start"]
    },
    {
      "before": ["v", "a", "A"],
      "commands": ["vscode-textobjects.select.previous.outer.parameters.start"]
    },
    {
      "before": ["v", "a", "C"],
      "commands": ["vscode-textobjects.select.previous.outer.comment.start"]
    },
    {
      "before": ["v", "i", "C"],
