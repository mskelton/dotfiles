{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(awk:*)",
      "Bash(bun add:*)",
      "Bash(bun install:*)",
      "Bash(bun remove:*)",
      "Bash(bun update:*)",
      "Bash(cat:*)",
      "Bash(chmod:*)",
      "Bash(cp:*)",
      "Bash(diff:*)",
      "Bash(echo:*)",
      "Bash(find:*)",
      "Bash(git:*)",
      "Bash(go build:*)",
      "Bash(go get:*)",
      "Bash(go mod tidy:*)",
      "Bash(go test:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(jq:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(npm install:*)",
      "Bash(pnpm add:*)",
      "Bash(pnpm dedupe:*)",
      "Bash(pnpm install:*)",
      "Bash(pnpm remove:*)",
      "Bash(pnpm update:*)",
      "Bash(rg:*)",
      "Bash(sed:*)",
      "Bash(tail:*)",
      "Bash(touch:*)",
      "Bash(wc:*)",
      "Bash(which:*)",
      "Bash(xargs:*)",
      "Bash(yarn add:*)",
      "Bash(yarn dedupe:*)",
      "Bash(yarn install:*)",
      "Bash(yarn lint:*)",
      "Bash(yarn remove:*)",
      "Bash(yarn up:*)",
      "WebFetch",
      "WebSearch"
    ],
    "deny": ["Bash(rmdir:*)", "Bash(rm -rf:*)"]
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Funk.aiff"
          }
        ]
      }
    ],
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Funk.aiff"
          }
        ]
      }
    ]
  },
  "alwaysThinkingEnabled": true
}
