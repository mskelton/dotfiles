con continue
diff !git diff "$(gt parent)"..HEAD
fix !git add . && gt continue
go !gt modify --all && git push --force-with-lease
linear !linear
ll log long
ls log short
ma modify --all
main !gt sync -f && gt checkout $(git default)
name rename
new !gt create -am "$2"
pr submit --stack --publish --cli
prd submit --stack --draft --cli
resatck restack
sa !gt modify --all && gt squash -n 2>/dev/null
squash !gt track -q && gt squash --no-edit
ss submit --stack
su submit --stack --update-only
sync sync -f
untracked !untracked
