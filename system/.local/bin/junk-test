#!/usr/bin/env bash
# Generate a junk test for testing reporters

cat <<EOF >playwright/tests/foo.spec.ts
import { expect, test } from '@playwright/test'

for (let i = 0; i < 500; i++) {
  test(\`test \${i}\`, () => {
    test.fixme(i % 4 === 0)

    if (i % 73 === 0) {
      expect(true).toBe(false)
    } else {
      expect(true).toBe(true)
    }
  })
}
EOF
