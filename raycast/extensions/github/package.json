{
  "name": "github",
  "description": "View GitHub repos, PRs, issues, and more.",
  "author": "mskelton",
  "license": "ISC",
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  },
  "title": "GitHub",
  "icon": "github.png",
  "commands": [
    {
      "name": "search-repos",
      "title": "Search Repositories",
      "subtitle": "GitHub",
      "description": "Search your repositories by name.",
      "mode": "view"
    },
    {
      "name": "my-prs",
      "title": "My Pull Requests",
      "subtitle": "GitHub",
      "description": "View pull requests you have opened.",
      "mode": "view"
    },
    {
      "name": "my-issues",
      "title": "My Issues",
      "subtitle": "GitHub",
      "description": "View issues you have created.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "type": "password",
      "required": true,
      "title": "Personal Access Token",
      "description": "Your GitHub personal access token.",
      "placeholder": "Enter your GitHub token."
    },
    {
      "name": "query",
      "type": "textfield",
      "required": false,
      "title": "Query",
      "description": "Search query to use for all commands.",
      "placeholder": "e.g. org:Microsoft"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.2",
    "@raycast/utils": "^1.0.6",
    "node-fetch": "^3.2.9"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.30.1",
    "react-devtools": "^4.25.0",
    "typescript": "^4.7.4"
  }
}
