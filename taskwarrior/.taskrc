# Taskwarrior program configuration file.

# Files
data.location=~/.task
# hooks.location=~/.config/task/hooks

# Misc
include dark-256.theme
news.version=2.6.0

# Reports
report.next.filter=status:pending -WAITING -BLOCKED limit:page

# Context
context.home.read=project != Analyzer
context.analyzer.read=project:Analyzer
context.analyzer.write=project:Analyzer
context=home
